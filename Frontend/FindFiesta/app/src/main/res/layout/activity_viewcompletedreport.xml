<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_viewcompletedreport"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/colorPrimary">

    <TextView
        android:id="@+id/reportedUserText2"
        android:layout_width="280dp"
        android:layout_height="134dp"
        android:text="Reported User"
        android:textAlignment="center"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.132" />

    <Button
        android:id="@+id/backButton101"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="?attr/colorSecondary"
        android:text="Back"
        app:layout_constraintBottom_toTopOf="@+id/reportedUserText2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.052"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.39" />

    <TextView
        android:id="@+id/reportcommentsText2"
        android:layout_width="274dp"
        android:layout_height="136dp"
        android:layout_marginTop="28dp"
        android:text="No reporter comments"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/moderatorComments"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reportedUserText2"
        app:layout_constraintVertical_bias="0.003" />

    <TextView
        android:id="@+id/moderatorComments"
        android:layout_width="280dp"
        android:layout_height="153dp"
        android:layout_marginBottom="244dp"
        android:text="No Moderator Comments"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/deleteCompletedReportButton"
        android:layout_width="186dp"
        android:layout_height="52dp"
        android:backgroundTint="?attr/colorSecondary"
        android:text="Delete Report"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/moderatorComments" />

    <TextView
        android:id="@+id/reviewedByText"
        android:layout_width="281dp"
        android:layout_height="28dp"
        android:text="reviewed by:"
        app:layout_constraintBottom_toTopOf="@+id/deleteCompletedReportButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/moderatorComments" />
</androidx.constraintlayout.widget.ConstraintLayout>